Script "GetAutoSaveTime" (void) NET
{
	int sae_savetimeCTR = GetUserCVAR(PlayerNumber(), "saehud_savetime");
	int t;
	t = 0;
	While(true)
	{
		Delay(35);
		AutosaveTime=StrParam(d:(sae_savetimeCTR-1)-(t/60), s:":", d:5-((t%60)/10), d:9-(t%10));
		t++;
	}
}


