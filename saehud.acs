					Until(alarmcnt[PlayerNumber()]>0)
					{
					PlaySound(ActivatorTID(),"DANGER", 6, 0.2);
					alarmcnt[PlayerNumber()]++;
					}
				}
		}
		Else
		{
			holex = 0;
			holey = 0;
			//ACS_NamedTerminate("dangeralarm",0);
			If(alarmcnt[PlayerNumber()] > 0 )
			{
			StopSound(ActivatorTID(),6);
			alarmcnt[PlayerNumber()] = 0;
			}
		}
		Delay(1);
	}
}

